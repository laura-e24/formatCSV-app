{
  "swagger": "2.0",
  "info": {
    "title": "FormatCSV - OpenAPI 2.0",
    "description": "description",
    "version": "0.1.9"
  },
  "servers": [
    {
      "url": "https://laura-e24.github.io/formatCSV-app/api"
    }
  ],
  "paths": {
    "/api/files/data": {
      "get": {
        "summary": "Finds all available files",
        "description": "Retrieves all available files in JSON format",
        "responses": {
          "200": {
            "description": "Files were retrieved and formatted successfully",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/File"
                  }
                }
              }
            }
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "File": {
        "type": "object",
        "properties": {
          "file": {
            "type": "string",
            "example": "test10.csv"
          },
          "lines": {
            "type": "array",
            "example": [
              {
                "text": "ruHiwzcRKUhbY",
                "number": "56877",
                "hex": "711b9dc1p423b8f9afc17830ae81b789"
              }
            ]
          }
        }
      }
    }
  }
}
